	private String uidx; //ȸ�� �Ϸù�ȣ
	private	String uid; //ȸ�� ���̵�
	private String upw; //ȸ�� ��й�ȣ
	private String uname; //ȸ�� �̸�
	private	String ubirth; //ȸ�� �������
	private String uphone; //ȸ�� ��ȭ��ȣ
	private String uaddress; //ȸ�� �ּ� ex) 55555 ���ֽ� ������ ��õ��
	private String uaddressD; //ȸ�� �� �ּ�
	private String uemail; //ȸ�� �̸���
	private Date uinsertdate; //ȸ�� ����� YYYY-MM-DD
	private Date uupdatedate; //ȸ�� ������
	private	String udeleteyn; //ȸ�� Ż�𿩺� Y or N
	private String ugrade;// ȸ�� ��� ������M ȸ��U	
	public String getUidx() {
		return uidx;
	}
